{
	"type": "commonjs",
	"name": "on-the-way",
	"version": "1.0.0",
	"description": "Student Counselling System for IIITDMJ",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js",
		"client": "npm start --prefix client",
		"client-install": "cd client && npm install",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"install-all": "npm install && cd client && npm install",
		"build": "cd client && npm run build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"concurrently": "^8.2.2",
		"nodemon": "^3.0.1"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"jsonwebtoken": "^9.0.2",
		"mysql2": "^3.6.3"
	}
}
